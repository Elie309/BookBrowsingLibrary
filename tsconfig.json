{
    "compilerOptions": {
        "outDir": "dist",
        "declaration": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES6",
        "rootDir": "src",
        "allowJs": true,
        "esModuleInterop": true,
    },
    "exclude": [
        "src/**/*.test.ts",
        "src/tests/**/*.ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    
}